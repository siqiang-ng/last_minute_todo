{
    "manifest_version": 3,
    "name": "Last Minute To Do",
    "description": "Last Minute To Do",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/icon_16.png",
            "32": "/images/icon_32.png",
            "48": "/images/icon_48.png",
            "128": "/images/icon_128.png"
        }
    },
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png" 
    },

    "background": {
        "service_worker": "background.js"
    },

    "permissions": ["storage"]
}